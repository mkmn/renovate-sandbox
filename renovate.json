{
  "dependencyDashboard": true,
  "enabledManagers": [
    "dockerfile",
    "nodenv",
    "custom.regex",
    "circleci"
  ],
  "extends": [
    "customManagers:dockerfileVersions"
  ],
  "packageRules": [
    {
      "commitMessageTopic": "Node.js",
      "matchDepNames": ["node"],
      "matchPackageNames": [
        "/(?:^|/)node$/"
      ],
      "versioning": "node"
    }
  ]
}
