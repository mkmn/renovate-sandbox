{
  "dependencyDashboard": true,
  "enabledManagers": [
    "custom.regex",
    "circleci",
    "nodenv"
  ],
  "extends": [
    "customManagers:dockerfileVersions"
  ],
  "packageRules": [
    {
      "groupName": "Node.js",
      "commitMessageTopic": "Node.js",
      "matchDatasources": [
        "docker",
        "node-version"
      ],
      "matchPackageNames": [
        "/(?:^|/)node$/"
      ],
      "versioning": "node"
    }
  ]
}
